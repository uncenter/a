{
	"name": "markdown-it-kbd-better",
	"version": "1.2.0",
	"description": "Markdown-it syntax add-on for the <kbd> tag, but with tiny improvements",
	"keywords": [
		"markdown-it",
		"markdown-it-plugin",
		"kbd"
	],
	"homepage": "https://github.com/uncenter/markdown-it-kbd-better",
	"license": "MIT",
	"files": [
		"README.md",
		"LICENSE",
		"build/index.js",
		"build/index.d.ts"
	],
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/uncenter/markdown-it-kbd-better.git"
	},
    "np": {
        "cleanup": false
    },
	"scripts": {
		"build:transpile": "babel ./src --out-dir build --extensions '.ts'",
		"build:types": "tsc --project src",
		"lint": "npm-run-all lint:*",
		"lint:types": "tsc",
		"lint:style": "eslint --fix --ext .ts src",
		"build": "npm-run-all build:*",
		"test": "jest",
		"go": "npm run lint && npm run build && np"
	},
	"devDependencies": {
		"@babel/cli": "7.21.5",
		"@babel/core": "7.22.1",
		"@babel/preset-env": "7.22.2",
		"@babel/preset-typescript": "7.21.5",
		"@types/jest": "29.5.1",
		"@types/markdown-it": "12.2.3",
		"@typescript-eslint/eslint-plugin": "5.59.7",
		"@typescript-eslint/parser": "5.59.7",
		"babel-plugin-add-module-exports": "1.0.4",
		"eslint": "8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "29.5.0",
		"markdown-it-attrs": "4.1.6",
		"npm-run-all": "4.1.5",
		"prettier": "^2.8.8",
		"typescript": "5.0.4"
	},
	"dependencies": {
		"markdown-it": "^13.0.1"
	}
}
